    "homepage": "https://github.com/freestream/Api_Boolin",
    "keywords": ["boolin", "gh", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Samuelsson",
            "email": "samuelsson.anton@gmail.com"
        }
